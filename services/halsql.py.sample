[defaults]
hostname  = 'localhost'
port      = 1883
loglevel  = DEBUG

launch	 = halsql

functions = 'halacsfuncs.py'

[config:halsql]
host  =  'localhost'
port  =  3306
user  =  'root'
pass  =  '123'
dbname  =  'test'
targets = {
		 't1'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'Power': 'value',
							},
							{
							  'description' : 'bojler_power'
							}
				  ],
		 't2'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'Current': 'value',
							},
							{
							  'description' : 'bojler_current'
							}
				  ],
		 't3'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'Voltage': 'value',
							},
							{
							  'description' : 'bojler_voltage'
							}
				  ],
		 't4'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'Factor': 'value',
							},
							{
							  'description' : 'bojler_factor'
							}
				  ],
		 't5'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'Total': 'value',
							},
							{
							  'description' : 'bojler_total'
							}
				  ],
		 't6'   : [ 'test',
							{
							  'Time': 'timestamp',
							  'POWER_BIN': 'value',
							},
							{
							  'description' : 'bojler_enabled'
							}
				  ]
  }

[tele/+/SENSOR]
targets = halsql:t1, halsql:t2, halsql:t3, halsql:t4, halsql:t5
alldata = powerBinFunc()

[tele/+/STATE]
targets = halsql:t6
alldata = powerBinFunc()
